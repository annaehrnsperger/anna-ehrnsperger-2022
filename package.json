{
  "name": "anna-ehrnsperger",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "watch:js": "esbuild ./src/scripts/main.js --outfile=public/main.js --bundle --watch",
    "build:js": "esbuild ./src/scripts/main.js --outfile=public/main.js --bundle --minify",
    "watch:css": "TAILWIND_MODE=watch postcss src/styles/index.css -o public/index.css --verbose -w",
    "build:css": "NODE_ENV=production postcss src/styles/index.css -o public/index.css --verbose",
    "clean": "rm -rf public",
    "build": "run-s clean build:*",
    "start": "run-p clean watch:*",
    "netlify": "netlify dev"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "no-console": 1,
      "no-new": 0,
      "class-methods-use-this": 0
    }
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "autoprefixer": "^10.3.7",
    "babel-eslint": "^10.1.0",
    "cssnano": "^5.0.2",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-wesbos": "^1.0.1",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.9",
    "postcss-cli": "^9.0.1",
    "postcss-modules": "^4.0.0",
    "prettier": "^2.3.0",
    "tailwindcss": "^3.0.13"
  },
  "dependencies": {
    "@dogstudio/highway": "^2.2.1",
    "@sanity/block-content-to-html": "^2.0.0",
    "@sanity/client": "^2.23.2",
    "@sanity/image-url": "^1.0.1",
    "esbuild": "^0.14.11",
    "groq": "^2.13.0",
    "gsap": "^3.9.1",
    "hls.js": "^1.1.3",
    "lazysizes": "^5.3.2",
    "postcss-import": "^14.0.2",
    "smoothscroll-polyfill": "^0.4.4"
  }
}
